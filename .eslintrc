{
  "root": true,
  "env": {
    "node": true,
    "jest": true
  },
  "extends": [
    "plugin:vue/essential",
    "eslint:recommended"
  ],
  "rules": {
    "no-prototype-builtins": "off",
    "semi": ["error", "never"],
    "object-curly-spacing": ["error", "always"]
  },
  "parserOptions": {
    "parser": "babel-eslint"
  }
}
